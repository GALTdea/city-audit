<div class="page-header">
  <%= link_to items_path, class: 'btn btn-default' do %>
    All Items
  <% end %>
  <%= link_to edit_item_path(@item), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show item</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @item.title %></dd>

  <dt>Votes:</dt>
  <dd><%= @item.votes %></dd>

  <dt>Resume:</dt>
  <dd><%= @item.resume %></dd>

  <dt>Link:</dt>
  <dd><%= @item.link %></dd>

  <dt>Region:</dt>
  <dd><%= @item.region %></dd>

  <dt>Rating:</dt>
  <dd><%= @item.rating %></dd>

  <dt>User:</dt>
  <dd><%= @item.user %></dd>

</dl>
