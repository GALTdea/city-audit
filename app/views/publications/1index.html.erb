


<div class="row">
  <div class="col-sm-6">
    <h1>Publications</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_publication_path, class: 'btn btn-primary' do %>
    Add New Publication
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Title</th>
            <th>Likes</th>
            <th>Resume</th>
            <th>Link</th>
            <th>Journalist</th>
            <th>Publisher</th>
            <th>Rating</th>
            <th>User</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @publications.each do |publication| %>
        <%= content_tag :tr, id: dom_id(publication), class: dom_class(publication) do %>
          

                      <td><%= publication.title %></td>
                      <td><%= publication.likes %></td>
                      <td><%= publication.resume %></td>
                      <td><%= publication.link %></td>
                      <td><%= publication.journalist %></td>
                      <td><%= publication.publisher %></td>
                      <td><%= publication.rating %></td>
                      <td><%= publication.user %></td>
          
          
            <td><%= link_to 'Show', publication %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>



