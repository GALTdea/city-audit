<div class="page-header">
  <%= link_to publications_path, class: 'btn btn-default' do %>
    All Publications
  <% end %>
  <%= link_to edit_publication_path(@publication), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show publication</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @publication.title %></dd>

  <dt>Likes:</dt>
  <dd><%= @publication.likes %></dd>

  <dt>Resume:</dt>
  <dd><%= @publication.resume %></dd>

  <dt>Link:</dt>
  <dd><%= @publication.link %></dd>

  <dt>Journalist:</dt>
  <dd><%= @publication.journalist %></dd>

  <dt>Publisher:</dt>
  <dd><%= @publication.publisher %></dd>

  <dt>Rating:</dt>
  <dd><%= @publication.rating %></dd>

  <dt>User:</dt>
  <dd><%= @publication.user %></dd>

</dl>
